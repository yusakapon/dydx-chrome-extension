{
  "manifest_version": 3,
  "name": "dYdX Trade Support Extension",
  "description": "This extension is a tool to help you trade on the Ethereum Layer-2 dex 'dYdX'.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://trade.dydx.exchange/trade/*"],
      "js": ["loader.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/chunk-vendors.js", "js/app.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
