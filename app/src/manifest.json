{
  "manifest_version": 3,
  "name": "dYdX Trade Support Extension",
  "description": "This extension is a tool to help you trade on the Ethereum Layer-2 dex 'dYdX'.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://trade.dydx.exchange/trade/*",
        "https://trade.stage.dydx.exchange/trade/*"
      ],
      "js": ["loader.js"],
      "css": ["css/app.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/chunk-vendors.js", "js/app.js", "css/app.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
